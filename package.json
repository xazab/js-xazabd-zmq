{
  "name": "@xazab/xazabd-zmq",
  "version": "1.2.0",
  "description": "Xazab Client Library to connect and subscribe to Xazab Core (xazabd)'s ZMQ topics",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run test:unit",
    "test:unit": "mocha 'lib/**/*.spec.js'",
    "lint": "eslint .",
    "check-package": "npm run check-package:name && npm run check-package:version",
    "check-package:name": "test $(jq -r .name package.json) = $(jq -r .name package-lock.json)",
    "check-package:version": "test $(jq -r .version package.json) = $(jq -r .version package-lock.json)"
  },
  "contributors": [
    {
      "name": "Alex Werner",
      "email": "alex@dash.org",
      "url": "https://alex-werner.github.io/"
    }
  ],
  "keywords": [
    "xazab",
    "zmq"
  ],
  "dependencies": {
    "@types/node": "^14.11.2",
    "zeromq": "^6.0.0-beta.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xazab/js-xazabd-zmq.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xazab/js-xazabd-zmq/issues"
  },
  "homepage": "https://github.com/xazab/js-xazabd-zmq#readme"
}
